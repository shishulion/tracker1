generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       BigInt  @unique
  username String  @unique
  Habs     Habs[]
  hustle   BigInt
  group_id BigInt
}

model Habs {
  id     String  @id @default(cuid())
  title  String?
  format Typis
  userId BigInt
  User   User?   @relation(fields: [userId], references: [id])
}

enum Typis {
  time
  count
  tracka
  bububu
}
